<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
		<%@include file="../../util/head.jsp"%>
		<title>告警短信通知</title>
	</head>
	<body>
		<div>
			<form id="coverState" action="" method="post">
				<table class="condition">
					<tr>
						<td>接收短信时间：</td>
					</tr>
					<tr>
						<td width="8%">开始时间：</td>
						<td width="16%">
							<div class="condition-text-container">
								<input class="condition-text condition" type="text" value="" name=START_TIME id="task_start_time" 
									onClick="WdatePicker({startDate:'%y-%M-{%d-7}'});" />
							</div>
						</td>
						<td width="8%">至：</td>
						<td width="16%">
							<div class="condition-text-container">
								<input class="condition-text condition" type="text" value="" name=START_TIME id="task_start_time" 
									onClick="WdatePicker({startDate:'%y-%M-{%d-7}'});" />
							</div>
						</td>
						<td>
							<div class="btn-operation" onClick="searchData()">确定</div>
						</td>
					</tr>
				</table>
			</form>
		</div>
	</body>
</html>