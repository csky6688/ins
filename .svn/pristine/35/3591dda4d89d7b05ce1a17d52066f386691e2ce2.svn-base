<%@ page language="java" import="java.util.*" pageEncoding="utf-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<%@include file="../../util/head.jsp"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<script type="text/javascript" src="http://api.map.baidu.com/api?v=2.0&ak=4E1fb44d64c4f75d50a0ec54abff48ff"></script>
<head>
<style>
	tr,td {
		padding: 10px 10px 10px 10px;
	}
	.custom_field{
		font-size:14px;
	}
</style>
</head>
<body style="padding: 3px; border: 0px">
<div>
	<form id="jobId">
		<input type="hidden" name="check_detail_id" value="${ROOM_ID}" />
		<div>
			<table width="750" border="0" align="center" cellpadding="0" cellspacing="20px" bgcolor="#f4f4f4">
			<tr>
			<td><label>机房名称：</label> </td>
			<td><label class="custom_field">${ROOM_NAME}</label></td>	
			<td><label>机房类型：</label> </td>
			<td><label class="custom_field">${ROOM_TYPE_NAME}</label></td>	
			</tr>
			<tr>
			<td><label>巡检人：</label> </td>
			<td><label class="custom_field">${STAFF_NAME}</label></td>	
			<td><label>地址：</label> </td>
			<td><label class="custom_field">${ADDRESS}</label></td>	
			</tr>
			<tr>
			<td><label>经度：</label> </td>
			<td><label class="custom_field">${LONGITUDE}</label></td>	
			<td><label>纬度：</label> </td>
			<td><label class="custom_field">${LATITUDE}</label></td>	
			</tr>
			<tr>
			<td><label>地区：</label> </td>
			<td><label class="custom_field">${AREA_NAME}</label></td>	
			<td><label>区县：</label> </td>
			<td><label class="custom_field">${SON_AREA_NAME}</label></td>	
			</tr>
			<tr>
			<td><label>创建人：</label> </td>
			<td><label class="custom_field">${CREATE_BY}</label></td>	
			<td><label>创建时间：</label> </td>
			<td><label class="custom_field">${CREATE_DATE}</label></td>	
			</tr>
			<tr>
			<td><label>修改人：</label> </td>
			<td><label class="custom_field">${MODIFY_BY}</label></td>	
			<td><label>修改时间：</label> </td>
			<td><label class="custom_field">${MODIFY_DATE}</label></td>	
			</tr>
			<tr>
			<td><label>机房编码：</label> </td>
			<td colspan="3"><label class="custom_field">${ROOM_CODE}</label></td>	
			</tr>
			</table>
		</div>
	</form>
</div>
</body>