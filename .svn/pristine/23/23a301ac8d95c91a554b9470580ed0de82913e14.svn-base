    width: 150px;
    padding-left: 3px;
    /*padding-top: 1px;*/
}

.condition-text {
	width: 145px;
	font-size: 12px;
	border: 0px;
	margin-left: 3px;
	/*margin-top: 3px;*/
}

.condition-select {
	/*width: 154px;
	border: 1px solid;
	border-color: #cccccc;*/
