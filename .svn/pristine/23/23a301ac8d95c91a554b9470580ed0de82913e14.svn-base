}

.btn-operation-container {
	/*****  liutaiping edit
	width: 97%;
	height: 30px;
	padding-top: 10px;
	padding-left: 26px;
	padding-right: 36px;***/
	
	width: 97%;
	height: 30px;
	padding-top: 5px;
	padding-left: 0px;
	padding-right: 0px;
}

.panel-header {
	display: none;
	border: 0px;
	background: none;
	filter: none;
}

.window {
	border-color: #cccccc;
}

.window-header {
	display: block;
	height: 15px;
}

.panel-body {
	background-color: white;
}

.window .window-body {
	border: 0px solid;
}

.panel-title {
	font-size: 15px;
	font-weight: bold;
	padding-top: 2px;
	height: 21px;
	background: none;
}

.panel-tool {
	width: 22px;
	height: 22px;
	margin-right: 3px;
}

.panel-tool a {
	width: 20px;
	height: 20px;
	filter: none;
}

.panel-tool a.panel-tool-min {
	display: none;
}

.panel-tool a.panel-tool-max {
	display: none;
}

.panel-tool-close {
	width: 20px;
	height: 20px;
	background: url("../images/new/_0079_btn_close_normal.png") 0px 0px
		no-repeat;
}

.panel-tool a.panel-tool-close:HOVER {
	background: url("../images/new/_0080_btn_close_pressed.png") 0px 0px
		no-repeat;
}

.condition {
	width: 90%;
	border: 0px solid;
}

.condition tr td {
	font-size: 13px;
}

.condition-container {
	width: 100%;
	/***  liutaiping del
	height: 48px;
***/
	padding-top: 0px;
	padding-left: 0px;

}

.condition-text-container {
	/*width: 154px;
	height: 30px;
	background-image: url("../images/new/_0028_input.png");*/
