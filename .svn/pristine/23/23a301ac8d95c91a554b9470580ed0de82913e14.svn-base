.validatebox-invalid {
	border: 0px solid;
	border-color: transparent;
	background-color: transparent;
}

.datebox-button a {
	filter: none;
	opacity: 1;
}

.datebox {
	width: 145px;
	font-size: 12px;
	border: 0px;
	margin-left: 3px;
	/*margin-top: 3px;*/
}

.combo{
	display: list-item;	
}

.common-table {
	border-collapse: collapse;
	font-size: 13px;
	text-align: center;
	border: 1px solid;
	border-color: #cccccc;
	
}

.common-table thead tr {
	height: 25px;
	background-color: #ebebeb;
	border: 1px dotted;
	border-color: #cccccc;

}

.common-table tbody tr {
	height: 25px;
	border: 1px dotted;
	border-color: #cccccc;
	
}

.common-table  td,.common-table  th {
	
	border: 1px dotted #cccccc;
	border-color: #cccccc;
	text-align: center;
	width:25px

}

